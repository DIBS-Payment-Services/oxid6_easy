{
	"name": "nets/netseasyoxid6",
	"version": "1.2.0",
	"description": "Nets Easy Checkout module for oxid 6",
	"homepage": "https://nets.eu",
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	},
	"scripts": {
		"test": [
			"./vendor/bin/phpunit tests"
		],
		"test:coverage-html": [
			"@putenv XDEBUG_MODE=coverage",
			"./vendor/bin/phpunit --coverage-html coverage/ tests"
		]
	}
}